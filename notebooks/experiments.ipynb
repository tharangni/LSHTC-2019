    "import sys\n",
    "sys.path.append(\"..\")\n",
    "\n",
    "from scripts.src.hierarchy import *\n",
    "from scripts.src.label_utils import *\n",
    "\n",
    "import matplotlib.pyplot as plt\n",
    "import networkx as nx\n",
    "%matplotlib inline"
   ]
  },
  {
   "cell_type": "raw",
   "metadata": {},
   "source": [
    "with open(\"../large_lshtc_dataset.tar/large_lshtc_dataset/cat_hier.txt\", \"r\") as f:\n",
    "    lines = f.readlines()\n",
    "\n",
    "fresh = set()\n",
    "    \n",
    "for i, line in enumerate(tqdm(lines)):\n",
    "    el = line.strip().split(\" \")\n",
    "    il = list(map(int, el))\n",
    "    \n",
    "    for j in range(len(il)-1):\n",
    "        fresh.add((il[j], il[j+1]))\n",
    "fresh = list(fresh)"
   ]
  },
  {
   "cell_type": "raw",
   "metadata": {},
   "source": [
    "o = nx.from_edgelist(fresh)"
   ]
  },
  {
   "cell_type": "raw",
   "metadata": {},
   "source": [
    "nx.write_edgelist(o, \"../large-lshtc/pair_cat_hier.txt\")"
   ]
  },
